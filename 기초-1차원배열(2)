#include <stdio.h>

//번호를 n번 무작위로 불렀을 때, 각 번호(1 ~ 23)가 불린 횟수를 각각 출력해보자.
int main(void) {
  int n, i, t;//n = 부를 횟수,
  int a[24]={};
  
  scanf("%d", &n);
  
  for(i = 1; i <= n; i++)
    {
      scanf("%d", &t);
      a[t] += 1;//번호가 나온 횟수 저장.
    }
    for(i = 1; i <= 23; i++)
    printf("%d ", a[i]);//1~23까지 번호가 불린 횟수 출력.

  
  return 0;
}
