#include <stdio.h>

//번호(1~23번)를 n번 무작위로 불렀을 때,가장 작은 번호 출력하자.
int main(void) {
  int n, i, t; // n = 부를 횟수
  int min = 24; // 가장 큰 값 초기화

  scanf("%d", &n);

  for(i = 1; i <= n; i++){
    scanf("%d ", &t);
    if (t < min) // 현재 입력된 번호가 min보다 작으면 min 값 갱신
      min = t;
    
  }

  printf("%d", min);
    
  return 0;
}
